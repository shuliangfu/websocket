{
  "name": "@dreamer/websocket",
  "version": "1.0.0-beta.5",
  "license": "MIT",
  "exports": {
    ".": "./src/mod.ts",
    "./client": "./src/client/mod.ts"
  },
  "publish": {
    "include": [
      "src/mod.ts",
      "src/client/mod.ts",
      "LICENSE.md",
      "README.md",
      "TEST_REPORT.md"
    ],
    "exclude": [
      "**/*.test.ts",
      "**/*.spec.ts",
      "**/test/**",
      "**/tests/**"
    ]
  },
  "lint": {
    "include": [
      "src/"
    ],
    "rules": {
      "tags": [
        "recommended"
      ],
      "exclude": [
        "no-explicit-any"
      ]
    }
  },
  "imports": {
    "@dreamer/crypto": "jsr:@dreamer/crypto@1.0.0-beta.2",
    "@dreamer/test": "jsr:@dreamer/test@^1.0.0-beta.12",
    "@dreamer/runtime-adapter": "jsr:@dreamer/runtime-adapter@1.0.0-beta.14",
    "@dreamer/logger": "jsr:@dreamer/logger@^1.0.0-beta.2"
  },
  "test": {
    "permissions": {
      "net": true
    }
  }
}
